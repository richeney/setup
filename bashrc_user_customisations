export EDITOR=vi
export PS1='\[\033[01;32m\]\w\[\033[01;33m\]$(__git_ps1 " (%s)") \[\033[01;37m\]\$ '
export PATH=$PATH:/usr/local/bin:/usr/local/go/bin:~/go/bin

alias ll='ls -l'
alias re='clear'

title() {
 echo -ne "\033]0;${1:-${PWD##*/}}\a"
}

set -o vi

export JQ_COLORS="1;90:1;35:1;35:0;36:0;33:1;37:1;37"
